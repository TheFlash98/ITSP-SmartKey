<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ankitbohra.smartkey">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".UserLoginActivity" />
        <activity android:name=".DemoActivity" />
        <activity android:name=".AddAdminActivity" />
        <activity android:name=".AddTempActivity" />
        <activity android:name=".StartTimeActivity" />
        <activity android:name=".EndTimeActivity" />
        <activity android:name=".Datepicker"></activity>
    </application>

</manifest>